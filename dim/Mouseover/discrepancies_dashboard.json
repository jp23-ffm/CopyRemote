{
  "dashboard": {
    "title": "Server Data Quality Dashboard",
    "widgets": [
      {
        "id": "overall_quality",
        "type": "gauge",
        "size": "large",
        "title": "Overall Data Quality",
        "metric": "percentage_clean",
        "thresholds": {
          "critical": 80,
          "warning": 95,
          "good": 100
        },
        "colors": {
          "critical": "#dc3545",
          "warning": "#ffc107",
          "good": "#28a744"
        },
		"link": "/discrepancies/servers/",
		"info": "Percentage of servers with no data quality issues detected"
      },
    {
       "id": "live_status_inconsistent",
        "type": "gauge",
        "size": "small",
        "title": "Live Status Inconsistencies",
        "metric": "live_status_inconsistent_count",
        "link": "/discrepancies/servers/?live_status_inconsistent=ko",
        "info": "Servers flagged OPERATIONAL in ServiceNow but not reporting as ALIVE"
      },
      {
        "id": "live_status_quality",
        "type": "gauge",
        "size": "small",
        "title": "Live Status",
        "metric": "missing_live_status_count",
        "link": "/discrepancies/servers/?live_status_ok=False",
        "info": "Servers with missing or invalid Live Status field"
      },
      {
        "id": "snow_status_quality",
        "type": "gauge",
        "size": "small",
        "title": "SNow Status",
        "metric": "missing_snow_status_count",
        "link": "/discrepancies/servers/?snow_status_ok=False",
        "info": "Servers with missing or invalid ServiceNow Status field"
      },
      {
        "id": "osshortname_quality",
        "type": "gauge",
        "size": "small",
        "title": "OS Shortname",
        "metric": "missing_osshortname_count",
        "link": "/discrepancies/servers/?osshortname_ok=False",
        "info": "Servers with missing or invalid OS Short Name"
      },
      {
        "id": "osfamily_quality",
        "type": "gauge",
        "size": "small",
        "title": "OS Family",
        "metric": "missing_osfamily_count",
        "link": "/discrepancies/servers/?osfamily_ok=False",
        "info": "Servers with missing or invalid OS Family"
      },
      {
        "id": "infraversion_quality",
        "type": "gauge",
        "size": "small",
        "title": "Infra Version",
        "metric": "missing_infraversion_count",
        "link": "/discrepancies/servers/?infraversion_ok=False",
        "info": "Servers with missing or invalid Infrastructure Version"
      },
      {
        "id": "ipaddress_quality",
        "type": "gauge",
        "size": "small",
        "title": "IP Address",
        "metric": "missing_ipaddress_count",
        "link": "/discrepancies/servers/?ipaddress_ok=False",
        "info": "Servers with missing or invalid IP Address"
      },
      {
        "id": "app_auid_value_quality",
        "type": "gauge",
        "size": "small",
        "title": "Application AUID",
        "metric": "missing_app_auid_value_count",
        "link": "/discrepancies/servers/?app_auid_value_ok=False",
        "info": "Servers with missing or invalid Application AUID"
      },
      {
        "id": "app_name_value_quality",
        "type": "gauge",
        "size": "small",
        "title": "Application Name",
        "metric": "missing_app_name_value_count",
        "link": "/discrepancies/servers/?app_name=MISSING",
        "info": "Servers with missing or invalid Application Name"
      },
      {
        "id": "machine_type_quality",
        "type": "gauge",
        "size": "small",
        "title": "Machine Type",
        "metric": "missing_machine_type_count",
        "link": "/discrepancies/servers/?machine_type_ok=False",
        "info": "Servers with missing or invalid Machine Type"
      },
      {
        "id": "manufacturer_quality",
        "type": "gauge",
        "size": "small",
        "title": "Manufaturer",
        "metric": "missing_manufacturer_count",
        "link": "/discrepancies/servers/?manufacturer_ok=False",
        "info": "Servers with missing or invalid Manufacturer"
      },
      {
        "id": "country_quality",
        "type": "gauge",
        "size": "small",
        "title": "Country",
        "metric": "missing_country_count",
        "link": "/discrepancies/servers/?country_ok=False",
        "info": "Servers with missing or invalid Country"
      },
      {
        "id": "region_quality",
        "type": "gauge",
        "size": "small",
        "title": "Region",
