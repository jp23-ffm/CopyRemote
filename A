# Au dÃ©but de generate_charts
seen_server_ids = set()
unique_servers = []
for server in servers:
    if server.SERVER_ID not in seen_server_ids:
        seen_server_ids.add(server.SERVER_ID)
        unique_servers.append(server)

# Ensuite travailler avec unique_servers
aggregated_chart = {
    'servers': [getattr(s, field_key) for s in unique_servers],
    # ...
}
